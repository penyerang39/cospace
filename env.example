# TinaCMS Configuration
TINA_PUBLIC_IS_LOCAL=true

# GitHub Configuration (for production)
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_token_here
GITHUB_OWNER=your_github_username
GITHUB_REPO=your_repository_name
GITHUB_BRANCH=main

# NextAuth Configuration
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# Vercel KV / Upstash Redis Configuration (for production)
KV_REST_API_URL=your_vercel_kv_url_here
KV_REST_API_TOKEN=your_vercel_kv_token_here

# Vercel Environment Variables (automatically set by Vercel)
# VERCEL_GIT_REPO_OWNER=your_github_username
# VERCEL_GIT_REPO_SLUG=your_repository_name
# VERCEL_GIT_COMMIT_REF=main

# Debug mode (optional)
DEBUG=false
