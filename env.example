# TinaCMS Configuration
TINA_PUBLIC_IS_LOCAL=true

# TinaCMS Auth Token (REQUIRED for production)
# Generate a secure random token - this is what you'll enter in the TinaCMS login prompt
TINA_TOKEN=your_secure_token_here

# GitHub Configuration (for production)
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_token_here
GITHUB_OWNER=your_github_username
GITHUB_REPO=your_repository_name
GITHUB_BRANCH=main

# Upstash Redis Configuration (REQUIRED for production)
# TinaCMS uses Redis as its database layer for content management
UPSTASH_REDIS_REST_URL=your_upstash_redis_url_here
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token_here

# Vercel Environment Variables (automatically set by Vercel)
# VERCEL_GIT_REPO_OWNER=your_github_username
# VERCEL_GIT_REPO_SLUG=your_repository_name
# VERCEL_GIT_COMMIT_REF=main

# Debug mode (optional)
DEBUG=false
